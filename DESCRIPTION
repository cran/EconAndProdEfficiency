Package: EconAndProdEfficiency
Type: Package
Title: Economic and Production Efficiency
Version: 0.1.0
Authors@R: c(person("Sudip Kumar", "Dutta ", role = c("aut","cph"),email = "Sudip.Dutta@icar.gov.in"),
               person("Tanuj", "Misra",  role = c("aut", "cre", "cph"), email = "tanujmisra102@gmail.com")
